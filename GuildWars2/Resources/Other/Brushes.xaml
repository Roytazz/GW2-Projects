<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="AccentColorTransparentBrush" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.RelativeTransform>
            <TransformGroup>
                <ScaleTransform CenterY="0.5" CenterX="0.5"/>
                <SkewTransform CenterY="0.5" CenterX="0.5"/>
                <RotateTransform Angle="90" CenterY="0.5" CenterX="0.5"/>
                <TranslateTransform/>
            </TransformGroup>
        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="#00FF0000" Offset="0.01" />
        <GradientStop Color="#FFD60D0D" Offset="0.2"/>
        <GradientStop Color="#FFD60D0D" Offset="0.8"/>
        <GradientStop Color="#00FF0000" Offset="0.99"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="FadeBrush" Color="#FFC5C5C5"/>
    <SolidColorBrush x:Key="Gw2RedDarkAccent" Color="#FF762222"/>
    <SolidColorBrush x:Key="Gw2RedLightAccent" Color="#FFE03636"/>
    <SolidColorBrush x:Key="GrayAccent" Color="#FFA2A2A2"/>
    <SolidColorBrush x:Key="BackgroundDark" Color="#FF272727"/>
</ResourceDictionary>